<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.itcast.calculate.mapper.NcdMapper">
    
    <!-- 查询NCD系数 -->
    <select id="queryNcd" resultType="cn.itcast.calculate.entity.Ncd">
        SELECT 
            claim_count,
            ncd_rate
        FROM ncd_table 
        WHERE claim_count = #{claimCount}
    </select>
    
</mapper>
